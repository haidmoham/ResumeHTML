public class queueQuestions {
  public static void main(String[] args) {
    Queue<String> queue = new ArrayDeque<>();
    queue.enqueue("K");
    queue.enqueue("P");
    queue.dequeue();
    queue.enqueue("G");
    String str = queue.getFront();
    queue.enqueue("R");
    queue.dequeue();
    queue.enqueue("V");
    queue.enqueue(str);
    System.out.println(queue.toString());
  }
}
